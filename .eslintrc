{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "class-methods-use-this": 0
  },
  "env": {
    "node": true,
    "mocha": true,
    "jest": true,
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false,
  },
  "extends": [
    "airbnb-base"
  ]
}
